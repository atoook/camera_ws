<!-- CHECK THE DEVICE
   $ls /dev/video*   -->
<launch>

	<arg name="video_deviceL" default="/dev/video10" />
	<arg name="video_deviceR" default="/dev/video12" />

	<arg name="image_width" default="640" />
	<arg name="image_height" default="480" />

	<!--  Start L handeye-->
	<node name="usb_camL" pkg="usb_cam" type="usb_cam_node" output="screen" >
		<param name="camera_info_url" value="file://$(find handeye_cam)/config/camera.yaml" />
		<param name="video_device" value="$(arg video_deviceL)" />
		<param name="image_width" value="$(arg image_width)" />
		<param name="image_height" value="$(arg image_height)"/>
		<param name="pixel_format" value="yuyv" />
		<param name="camera_frame_id" value="usb_cam" />
		<param name="io_method" value="mmap"/>
	</node>
	<node name="handeye_L" pkg="handeye_cam" type="handeye_detection.py" cwd="node" output='screen'>
		<param name="image_width" value="$(arg image_width)" />
		<param name="image_height" value="$(arg image_height)"/>
		<param name="LorR" value="L"/>
	</node>

	<!--  Start R handeye-->
	<node name="usb_camR" pkg="usb_cam" type="usb_cam_node" output="screen" >
		<param name="camera_info_url" value="file://$(find handeye_cam)/config/camera.yaml" />
		<param name="video_device" value="$(arg video_deviceR)" />
		<param name="image_width" value="$(arg image_width)" />
		<param name="image_height" value="$(arg image_height)"/>
		<param name="pixel_format" value="yuyv" />
		<param name="camera_frame_id" value="usb_cam" />
		<param name="io_method" value="mmap"/>
	</node>
	<node name="handeye_R" pkg="handeye_cam" type="handeye_detection.py" cwd="node" output='screen'>
		<param name="image_width" value="$(arg image_width)" />
		<param name="image_height" value="$(arg image_height)"/>
		<param name="LorR" value="R"/>
	</node>



</launch>